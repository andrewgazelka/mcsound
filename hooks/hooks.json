{
  "hooks": {
    "SessionStart": [
      {
        "matcher": "*",
        "hooks": [
          {
            "type": "command",
            "command": "nix run github:andrewgazelka/mcsound -- play random/door_open",
            "timeout": 10
          }
        ]
      }
    ],
    "SessionEnd": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "nix run github:andrewgazelka/mcsound -- play random/door_close",
            "timeout": 10
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "nix run github:andrewgazelka/mcsound -- play random/levelup",
            "timeout": 10
          }
        ]
      }
    ],
    "SubagentStop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "nix run github:andrewgazelka/mcsound -- play random/orb",
            "timeout": 10
          }
        ]
      }
    ],
    "Notification": [
      {
        "matcher": "*",
        "hooks": [
          {
            "type": "command",
            "command": "nix run github:andrewgazelka/mcsound -- play note/bell",
            "timeout": 10
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "Write|Edit",
        "hooks": [
          {
            "type": "command",
            "command": "nix run github:andrewgazelka/mcsound -- play block/amethyst/place1",
            "timeout": 10
          }
        ]
      },
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": "nix run github:andrewgazelka/mcsound -- play random/click",
            "timeout": 10
          }
        ]
      },
      {
        "matcher": "Task",
        "hooks": [
          {
            "type": "command",
            "command": "nix run github:andrewgazelka/mcsound -- play block/end_portal/endportal",
            "timeout": 10
          }
        ]
      }
    ],
    "PreToolUse": [
      {
        "matcher": "WebFetch|WebSearch",
        "hooks": [
          {
            "type": "command",
            "command": "nix run github:andrewgazelka/mcsound -- play block/end_portal/eyeplace1",
            "timeout": 10
          }
        ]
      }
    ]
  }
}
